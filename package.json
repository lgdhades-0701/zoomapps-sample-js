{
  "name": "hello-zoom-apps",
  "version": "0.0.0",
  "type": "module",
  "exports": "./server",
  "private": true,
  "scripts": {
    "start": "node app.js",
    "prestart": "npm run build",
    "dev-app": "npm run build -- --watch",
    "dev-server": "nodemon -q app.js",
    "build": "babel public --out-dir dist --ignore '**/vendor/*.js' --copy-files",
    "dev": "concurrently -kn dev- -c inverse.magenta,inverse.green npm:dev-*",
    "postprepare": "chmod +x ./gen-secrets.sh && $_",
    "predev": "docker compose up -d",
    "prepare": "npx husky install",
    "clean": "docker compose down -v --remove-orphans"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "compression": "^1.7.4",
    "concurrently": "^7.1.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "~4.17.3",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "https-localhost": "^4.7.1",
    "mongoose": "^6.2.3",
    "mongoose-encryption": "^2.1.2",
    "morgan": "~1.10.0",
    "nodemon": "^2.0.15",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "babel-watch": "^7.7.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "not dead",
    "not ie 11"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
